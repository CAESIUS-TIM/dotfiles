syntax enable
set cursorline
set expandtab
set hlsearch
set ignorecase
set incsearch
set laststatus=2
" not treat as decimal or hex
set nrformats= 
set number
set shiftwidth=4
set showmatch
set smartcase
set smartindent
set tabstop=4
set virtualedit=onemore
set visualbell
set wildmode=list:longest
set wrapscan
nnoremap j gj
nnoremap k gk
